{
  "name": "components-repro",
  "version": "0.0.1",
  "source": "src/index.ts",
  "main": "./lib/index.js",
  "types": "./lib/index.d.js",
  "exports": {
    "import": "./lib/index.modern.js",
    "require": "./lib/index.js"
  },
  "files": [
    "lib/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle build -f cjs,modern --jsx React.createElement",
    "build-tsc": "tsc"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "10 KB"
    },
    {
      "path": "lib/index.modern.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "react-dom": "17.0.2",
    "react": "17.0.2"
  },
  "devDependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "~11.6.0",
    "@mui/material": "~5.2.6",
    "@mui/system": "~5.2.6",
    "@types/react": "17.0.8",
    "microbundle": "~0.13.2",
    "typescript": "~4.2.4"
  },
  "dependencies": {
    "@mui/lab": "~5.0.0-alpha.62"
  }
}
